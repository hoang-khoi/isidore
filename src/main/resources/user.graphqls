type Query {
    user(id: ID): User
}

type Mutation {
    user(email: String!, name: InputUserName): User
}

type User {
    id: ID!
    email: String!
    name: UserName!
}

type UserName {
    baptismalName: String
    firstName: String!
    middleName: String
    lastName: String!
}

input InputUserName {
    baptismalName: String
    firstName: String!
    middleName: String
    lastName: String!
}
